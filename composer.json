{
    "name": "phpbbvietnam/facebookpixel",
    "type": "phpbb-extension",
    "description": "An extension that allows administrators to easily add their Facebook Pixel tracking code to their phpBB forum.",
    "homepage": "https://phpbbvietnam.com",
    "version": "1.0.0",
    "keywords": [
        "phpbb",
        "extension",
        "facebook",
        "pixel"
    ],
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "phpBBVietnam",
            "email": "phpbbvietnam.com@gmail.com",
            "homepage": "http://phpbbvietnam.com",
            "role": "Extensions Development Team"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "composer/installers": "~1.0.0",
        "phpbb/phpbb": ">=3.1.0"
    },
    "require-dev": {
        "phing/phing": "2.4.*"
    },
    "extra": {
        "display-name": "Facebook Pixel",
        "soft-require": {
            "phpbb/phpbb": ">=3.1.0"
        },
        "version-check": {
            "host": "phpbbvietnam.com",
            "directory": "/development/",
            "filename": "facebookpixel.json"
        }
    }
}